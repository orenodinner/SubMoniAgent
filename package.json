{
  "name": "pixelagent",
  "version": "0.1.0",
  "description": "PixelAgent desktop AI agent MVP",
  "main": "dist/main.js",
  "scripts": {
    "dev": "concurrently -k \"npm:dev:renderer\" \"npm:dev:main\"",
    "dev:renderer": "vite --config vite.renderer.config.ts",
    "dev:main": "cross-env VITE_DEV_SERVER_URL=http://localhost:5173 TS_NODE_PROJECT=tsconfig.main.json node ./scripts/start-electron.js",
    "build:renderer": "vite build --config vite.renderer.config.ts"
  },
  "dependencies": {
    "classnames": "2.5.1",
    "electron": "30.0.9",
    "nanoid": "^5.1.6",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "zustand": "4.5.4"
  },
  "devDependencies": {
    "@types/node": "20.17.10",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@vitejs/plugin-react": "4.3.4",
    "concurrently": "8.2.2",
    "cross-env": "7.0.3",
    "ts-node": "10.9.2",
    "typescript": "5.6.3",
    "vite": "5.4.10"
  }
}
